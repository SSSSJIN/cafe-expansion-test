<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
         "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kakaopage.expansion.dao.MainEventDAO">

  <select id="selectMainEvents" resultType="MainEventVO">
    SELECT ID, TITLE, IMAGE_URL AS IMAGE_URL, SORT_ORDER AS SORT_ORDER
      FROM MAIN_EVENT
     ORDER BY SORT_ORDER, ID
  </select>

</mapper>
